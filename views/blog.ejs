<script>
$('#Bloghcontent').html('')
    $.ajax({
        url:'/common/fetchBlogList',
        method:'get',
        success: function(res){
            console.log(res)
            if(res.length>0){
                for(i=0;i<res.length;i++){
                    
                    $('#Bloghcontent').append(`<div class="post-preview">
                                            <a href="/common/posts?id=${res[i].blog_id}">
                                                <h2 class="post-title">${res[i].blog_Title}</h2>
                                    
                                            </a>
                                            <p class="post-meta">
                                                Posted by
                                                <a href="#!">Admin</a>
                                                on ${res[i].CreatedOn}
                                            </p>
                                        </div>
                                        <!-- Divider-->
                                        <hr class="my-4" />`)
                }
            }
        }
    })
</script>
