            <script>

                $(document).ready(function () {




                    $.ajax({
                        url: '/user/fetch_refdetailsById',
                        method: 'get',
                        success: function (data) {


                            console.log(data)
                            $('#userList').DataTable({


                                data: data,
                                dom: 'Blfrtip',
                                buttons: [
                                {
                    extend: 'csvHtml5',
                    title: 'User List',                    
                    text: '<i class="bi bi-file-earmark-spreadsheet-fill"></i>' 
                },
                {
                    extend: 'pdfHtml5',
                    title: 'User List',
                    text: '<i class="bi bi-file-earmark-pdf-fill"></i>',
                    pageSize: 'A2' 
                },
                                ],
                                'columns': [

                                    {
                                        'data': 'userjoinedon'

                                    },
                                    {
                                        'data': 'username'

                                    }, {
                                        'data': 'useremail'
                                    },
                                    {
                                        'data': 'usermobile'
                                    }, {
                                        'data': 'userstate'
                                    }, {
                                        'data': 'userdist'
                                    }, {
                                        'data': 'userconst'
                                    }, {
                                        'data': 'userpin'
                                    }

                                ],



                            })


                        },
                        error: function (e) {
                            console.log(e)
                        }
                    })


                    $.ajax({
                        url: '/user/fetch_vendorrefdetailsById',
                        method: 'get',
                        success: function (data) {


                            console.log(data)
                            $('#vendorRefCount').html(data.length)
                            $('#vendorList').DataTable({


                                data: data,
                                dom: 'Blfrtip',
                                buttons: [
                                {
                    extend: 'csvHtml5',
                    title: 'Vendor List',                    
                    text: '<i class="bi bi-file-earmark-spreadsheet-fill"></i>' 
                },
                {
                    extend: 'pdfHtml5',
                    title: 'Vendor List',
                    text: '<i class="bi bi-file-earmark-pdf-fill"></i>',
                    pageSize: 'A2' 
                },
                                ],
                                'columns': [
                 
                                {
                     'data': 'vendorjoinedon'
    
                 },
                 {
                     'data': 'vendorname'
    
                 }, {
                     'data': 'vendoremail'
                 },
                 {
                     'data': 'vendormobile'
                 }, 
                 {
                     'data': 'vendorstate'
                 }, 
                 {
                     'data': 'vendordist'
                 }, 
                 {
                     'data': 'vendorconst'
                 }, 
                 {
                     'data': 'vendorpin'
                 }, 
                 {
                     'data': 'vendorbusinessname'
                 }, 
                 {
                     'data': 'vendorbusinesstype'
                 }
    
                 ]



                            })


                        },
                        error: function (e) {
                            console.log(e)
                        }
                    })




                    $("#share").jsSocials({
                        text: 'Sign Up on KARWAT click here',
                        url: $('#linkShare').attr('href'),

                        showLabel: false,
                        showCount: 'inside',
                        shares: ["email", {

                            share: "twitter",
                            logo: "fab fa-twitter fa-inverse"

                        },
                            {

                                share: "facebook",
                                logo: "fab fa-facebook-f fa-inverse"

                            }
                            ,
                            {

                                share: "linkedin",
                                logo: "fab fa-linkedin fa-inverse"

                            }


                            , {

                                share: "whatsapp",
                                logo: "fab fa-whatsapp fa-inverse"

                            }]

                    });
                })

            </script>



