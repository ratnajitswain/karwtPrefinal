                <script>

                    $(document).ready(function () {
                        $('#blogListSide').html('')
                        fetchBlogList()

                    })
                
                    function fetchBlogList(){
                        $('#blogListSide').html('')
                        $.ajax({
                            url: '/common/fetchBlogList',
                            method: 'get',
                            success: function (res) {
                                console.log(res)
                                if (res.length > 0) {
                                    for (i = 0; i < 20; i++) {

                                        $('#blogListSide').append(`
                    
                    <a href="/common/posts?id=${res[i].blog_id}">
                                                <h2 class="post-title">${res[i].blog_Title}</h2>
                                                <p>${res[i].CreatedOn}</p>
                                            </a>
                                            <hr class="my-6">
                    `)
                                    }
                                }
                            }
                        })
                    }
                    $('#blogsearch').on('keyup', function(){  
                        $('#blogListSide').html('')
                        if($('#blogsearch').val()==''||$('#blogsearch').val()==null){ 
                            $('#blogListSide').html('') 
                            fetchBlogList()
                        }
                        else{
                        $.ajax({
                            url: '/common/fetchBlogListSearch?blog='+$('#blogsearch').val(),
                            method: 'get',
                            success: function (res) {
                                console.log(res)
                                if (res.length > 0) {
                                    for (i = 0; i < 20; i++) {

                                        $('#blogListSide').append(`
                    
                    <a href="/common/posts?id=${res[i].blog_id}">
                                                <h2 class="post-title">${res[i].blog_Title}</h2>
                                    <p>${res[i].CreatedOn}</p>
                                            </a>
                                            <hr class="my-6">
                    `)
                                    }
                                }
                            }
                        })

                    }
                })

                </script>



