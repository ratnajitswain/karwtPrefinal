    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script>





    var ctx = document.getElementById('myChart').getContext('2d');

    $.ajax({  
        url:'/admin/userChart',
        method:'get',
        success: function(res){  
            var lebel = []
            var dt = []
            res.forEach(e => {
                lebel.push(e.date)
                dt.push(e.counted_leads)
            });
            console.log(dt)
            var myChart = new Chart(ctx, {
    type: 'line',
    data: {
        labels: [...lebel],
        datasets: [{
            label: 'No. of Users and Vendors',
            data: [...dt],
            backgroundColor: [
                'rgba(255, 99, 132, 0.2)',
                'rgba(54, 162, 235, 0.2)',
                'rgba(255, 206, 86, 0.2)',
                'rgba(75, 192, 192, 0.2)',
                'rgba(153, 102, 255, 0.2)',
                'rgba(255, 159, 64, 0.2)'
            ],
            borderColor: [
                'rgba(255, 99, 132, 1)',
                'rgba(54, 162, 235, 1)',
                'rgba(255, 206, 86, 1)',
                'rgba(75, 192, 192, 1)',
                'rgba(153, 102, 255, 1)',
                'rgba(255, 159, 64, 1)'
            ],
            borderWidth: 1
        }]
    },
    options: {
        scales: {
            y: {
                beginAtZero: true
            }
        }
    }
});
        }
    })



</script>
